## -*- coding: utf-8 -*-
<%inherit file="base.tmpl"/>
<%block name="content">
        <!--Body content-->
        <div class="postbox">
        <h1>${title}</h1>
        <ul class="unstyled">
        % for text, link in items:
            <li><a href="${link}">${text}</a>
        % endfor
        </ul>
        </div>
        <!--End of body content-->
</%block>
